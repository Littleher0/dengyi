        <script type="text/javascript">
          (function(id){
            var doms = Q('#'+id);
            if(doms.length){
              var _url;
              if(Q.browser.iPad){
                _url = doms.attr('data-url-pad');
              }else if(Q.browser.iPhone || Q.browser.android){
                _url = doms.attr('data-url-phone');
              }
              if(_url && _url.trim()){
                window.location.href = _url;
              }
            }
          })('widget-link-selector');
        </script>
